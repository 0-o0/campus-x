        "mp-weixin": {
          "disableScroll": true
        },
        "app-plus": {
          "bounce": "none"
        },
        "mp-alipay": {
          "allowsBounceVertical": "NO"
        },
        "mp-baidu": {
          "disableScroll": true
        }
      }
    }
  ],
